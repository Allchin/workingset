/* rwRate */
select a.VARIABLE_VALUE  QUERIES,b.VARIABLE_VALUE  COM_COMMIT ,
c.VARIABLE_VALUE COM_ROLLBACK
,
CONCAT(lEFT(
(
(a.VARIABLE_VALUE  )/ ( b.VARIABLE_VALUE + c.VARIABLE_VALUE ))
*1,8),'倍','') 
读写比
from  GLOBAL_STATUS a,GLOBAL_STATUS b,GLOBAL_STATUS c
where 
a.VARIABLE_NAME='QUERIES'   
and  b.VARIABLE_NAME='COM_COMMIT'
and c.Variable_name='COM_ROLLBACK';