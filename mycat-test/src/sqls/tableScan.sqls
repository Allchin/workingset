/* tableScan */
select a.VARIABLE_VALUE  HANDLER_READ_RND_NEXT,b.VARIABLE_VALUE  COM_SELECT ,
CONCAT(lEFT(
(
(a.VARIABLE_VALUE  )/ ( b.VARIABLE_VALUE ))
*1,8),'倍','') 
表扫描率
from  GLOBAL_STATUS a,GLOBAL_STATUS b
where 
a.VARIABLE_NAME='HANDLER_READ_RND_NEXT'   
and  b.VARIABLE_NAME='COM_SELECT';

 